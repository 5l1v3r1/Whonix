kill_old_instances() {
  trap "error_handler" ERR INT TERM

  local PID="`pgrep -f "Whonix is produced independently from the Tor"`" || true
  if [ "$PID" != "" ]; then
    kill -9 "$PID" || true
  fi
  
  local PID="`pgrep -f "Whonix is produced independently from the Tor"`" || true
  if [ "$PID" != "" ]; then
    kill -9 "$PID" || true
  fi
} 

