kill_old_instances() {
   trap "" ERR INT TERM
   
   local me="$$"
   local nnn="`pgrep whonixcheck`"
      
   for ii in `echo $nnn`; do
      if [ "$ii" -ne "$me" ]; then
         #echo "killing: $ii ... aux: `ps aux | grep $ii`"
         kill -9 $ii
      else
         true
         #echo "not killing me $ii .. aux: `ps au | grep ii`"
      fi
   done
}

