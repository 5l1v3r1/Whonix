#!/bin/bash

set -x

error_handler() {
   echo "
###################################
## chroot script: ERROR detected. #
###################################
"

   exit 1
} 

trap "error_handler" ERR INT TERM

sudo -u user crontab -l || true
sudo -u user crontab /usr/local/share/whonix/user_crontab
sudo -u user crontab -l


