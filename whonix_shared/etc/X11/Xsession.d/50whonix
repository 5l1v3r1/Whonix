## Whonix /etc/X11/Xsession.d/50whonix

#set -x

if [ -z "$KDEDIRS" ]; then
   export KDEDIRS=/usr/share/whonix/kde/
else
   export KDEDIRS=/usr/share/whonix/kde/:$KDEDIRS
fi

#sleep 10

#set +x

## End of Whonix /etc/X11/Xsession.d/50whonix

