## whonixsetup edits /etc/tor/torrc, therefore needs root.
## Feel free to disable it.
user host=NOPASSWD: /usr/bin/whonixsetup
user host=NOPASSWD: /usr/lib/whonix/whonix_repo_r_config_exists
