#!/bin/bash

port="9108"

if [ -f "/usr/share/aos/aos_workstation" ]; then
   ip="192.168.0.10"
elif [ -f "/usr/share/aos/aos_gateway" ]; then
   ip="127.0.0.1"
else
   error "/usr/local/bin/apt-get uwt wrapper could not determine if this is aos-Workstation or aos-Gateway."
   error "Please report this bug!"
fi

/usr/local/bin/uwt -t 5 -i "$ip" -p "$port" -c "/usr/bin/htpdate $*"

# End of aos /usr/local/bin/apt-get uwt wrapper.
